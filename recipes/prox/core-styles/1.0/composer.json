{
  "name": "prox/core-styles",
  "type": "library",
  "description": "Core styles and assets for Symfony projects",
  "version": "1.0",
  "require": {
    "php": ">=7.4",
    "symfony/form": "*",
    "symfony/asset": "*",
    "symfony/twig-bundle": "*",
    "twig/twig": "*",
    "symfony/ux-twig-component": "*",
    "symfony/webpack-encore-bundle": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "npm install",
      "npm run dev"
    ],
    "post-update-cmd": [
      "npm install",
      "npm run build"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.1.*"
    }
  }
}
